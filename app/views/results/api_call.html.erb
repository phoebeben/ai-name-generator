<div class="container">
  <h1 id="results-header" class="text-center font-bold">your results</h1>
  <div>
    <h5 class="text-center">select the names you like
      to keep them when you regenerate your results</h5>
  </div>

  <div class="card-container">
    <% @response.each do |result| %>
      <div class="mt-20 bg-slate-100 w-20">
        <p><%= result %></p>
      </div>
    <% end %>
  </div>
</div>
